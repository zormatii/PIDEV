<section class="artists-section section-padding" id="section_3">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 text-center mb-5">
                <h2 class="display-4 font-weight-bold text-primary">Create a New Event</h2>
                <p class="lead text-muted">Fill out the form below to create a new event.</p>
            </div>

            <div class="col-lg-8 col-md-10 col-12">
                {{ form_start(form, {'attr': {'novalidate': 'novalidate', 'class': 'needs-validation event-form'}}) }}

                <div class="mb-4">
                    {{ form_label(form.mail, 'Mail adress', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.mail, {'attr': {'class': 'form-control', 'placeholder': 'enter a mail adress for confirmation', 'rows': 4, 'required': 'required'}}) }}
                    <div class="invalid-feedback">Please provide a adress mail.</div>
                </div>

                <!-- Event Type Field -->
                <div class="mb-4">
                    {{ form_label(form.typeEvenement, 'Event Type', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.typeEvenement, {'attr': {'class': 'form-select', 'required': 'required'}}) }}
                    <div class="invalid-feedback">Please select an event type.</div>
                </div>

                <!-- Title Field -->
                <div class="mb-4">
                    {{ form_label(form.titre, 'Title', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.titre, {'attr': {'class': 'form-control', 'placeholder': 'Enter the event title', 'required': 'required'}}) }}
                    <div class="invalid-feedback">Please enter a valid title.</div>
                </div>

                <!-- Description Field -->
                <div class="mb-4">
                    {{ form_label(form.description, 'Description', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.description, {'attr': {'class': 'form-control', 'placeholder': 'Describe the event', 'rows': 4, 'required': 'required'}}) }}
                    <div class="invalid-feedback">Please provide a description.</div>
                </div>

                <!-- Number of Seats Field -->
                <div class="mb-4">
                    {{ form_label(form.nombre_de_places, 'Number of Seats', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.nombre_de_places, {'attr': {'class': 'form-control', 'type': 'number', 'min': '1', 'required': 'required'}}) }}
                    <div class="invalid-feedback">Please enter a positive number of seats.</div>
                </div>

                <!-- Start Date Field -->
                <div class="mb-4">
                    {{ form_label(form.date_debut, 'Start Date', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.date_debut, {'attr': {'class': 'form-control', 'type': 'datetime-local', 'required': 'required'}}) }}
                    <div class="invalid-feedback">Please select a valid start date.</div>
                </div>

                <!-- End Date Field -->
                <div class="mb-4">
                    {{ form_label(form.date_fin, 'End Date', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.date_fin, {'attr': {'class': 'form-control', 'type': 'datetime-local', 'required': 'required'}}) }}
                    <div class="invalid-feedback">End date must be after the start date.</div>
                </div>

                <!-- Image URL Field -->
                <div class="mb-4">
                    {{ form_label(form.url_image, 'Image URL', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.url_image, {'attr': {'class': 'form-control', 'placeholder': 'Upload an image'}}) }}
                </div>

                <!-- Location Field -->
                <div class="mb-4">
                    {{ form_label(form.lieu, 'Location', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.lieu, {'attr': {'class': 'form-control', 'placeholder': 'Enter the location', 'required': 'required'}}) }}
                    <div class="invalid-feedback">Location is required.</div>
                </div>

                <!-- Latitude and Longitude Fields (Creative Layout) -->
                <div class="row mb-4">
                    <div class="col-md-6">
                        {{ form_label(form.latitude, 'Latitude', {'label_attr': {'class': 'form-label'}}) }}
                        {{ form_widget(form.latitude, {'attr': {'class': 'form-control', 'type': 'number', 'step': 'any', 'required': 'required'}}) }}
                        <div class="invalid-feedback">Please enter a valid latitude.</div>
                    </div>
                    <div class="col-md-6">
                        {{ form_label(form.longitude, 'Longitude', {'label_attr': {'class': 'form-label'}}) }}
                        {{ form_widget(form.longitude, {'attr': {'class': 'form-control', 'type': 'number', 'step': 'any', 'required': 'required'}}) }}
                        <div class="invalid-feedback">Please enter a valid longitude.</div>
                    </div>
                </div>

                <!-- Status Field -->
                <div class="mb-4">
                    {{ form_label(form.statut, 'Status', {'label_attr': {'class': 'form-label'}}) }}
                    {{ form_widget(form.statut, {'attr': {'class': 'form-select'}}) }}
                </div>

                <!-- Buttons -->
                <div class="d-flex justify-content-between mt-5">
                    <button type="submit" class="btn btn-primary btn-lg">
                        <i class="fas fa-calendar-plus"></i> Create Event
                    </button>
                    <a href="{{ path('app_evenement_index') }}" class="btn btn-secondary btn-lg">
                        <i class="fas fa-arrow-left"></i> Back to List
                    </a>
                </div>

                {{ form_end(form) }}
            </div>
        </div>
    </div>
